#include <iostream>
#include <cmath>
using namespace std;

int reversed(int a) {
int rev = 0;
    while (a != 0) {
    int rem = a % 10;
    rev = rev * 10 + rem;
        a = a / 10;
}
    return rev;
}

int main() {
  int a;
  cout << "Enter a number: ";
  cin >> a;
  cout << reversed(a) << endl;
  return 0;
}

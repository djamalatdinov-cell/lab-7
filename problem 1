#include <iostream>
#include <cmath>
using namespace std;

   int dat(int x,int y) {
       if (y==0) {
           return 1;
       }
    else {
        return x*pow(x,(y-1));
    }
   }

int main () {
    int x,y;
    cout<<"Enter your x and y: ";
    cin>>x>>y;
      cout <<dat(x,y);
       return 0;
   }

#include <iostream>
#include <cmath>
using namespace std;

   bool prime(int x) {
     if (x<2) {
         return false;
     }
for (int i=2; i<=sqrt(x); i++) {
    if (x%i == 0) {
        return false;
    } else {
        return true;
    }
}
   }

int main () {
    int x;
    cout<<"Enter your x : ";
    cin>>x;
      cout <<prime(x);
       return 0;
   }
